# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page


GET     /                           controllers.HomeController.index


# CrearEst
GET     /crearEstudiant           controllers.HomeController.crearEstudiantesGet()
POST    /crearEstudiant           controllers.HomeController.crearEstudiantesPost()
#Quit√© las dos rutas extra.

#Listar
GET     /crearEliminareditar          controllers.HomeController.crearEliminareditar()

#Editar..
GET     /editarEstudiante/:id       controllers.HomeController.editarEstudianteGet(id: Long)
POST    /editarEstudiante/:id       controllers.HomeController.editarEstudiantePost(id: Long)

#Eliminar 
GET     /eliminarEstudiante/:id       controllers.HomeController.EliminarEstudiante(id: Long)
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
