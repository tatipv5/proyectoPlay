# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page

GET     /                           controllers.HomeController.index

# CrearEstudiantes
GET     /crearEstudiant           controllers.HomeController.crearEstudiantesGet()
POST    /crearEstudiant           controllers.HomeController.crearEstudiantesPost()
#Quit√© las dos rutas extra.

#Listar
GET     /listaEstudiantes        controllers.HomeController.listaEstudiantes()

#Editar..
GET     /editarEstudiante/:id       controllers.HomeController.editarEstudianteGet(id: Long)
POST    /editarEstudiante/:id       controllers.HomeController.editarEstudiantePost(id: Long)

#Eliminar 
GET     /eliminarEstudiante/:id       controllers.HomeController.EliminarEstudiante(id: Long)
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

#Buscar..NUEVO
GET     /buscarEstudiante/:id       controllers.HomeController.BuscarEstudianteGet(id: Long)
POST    /buscarEstudiante/:id       controllers.HomeController.BuscarEstudiantePost(id: Long)



#Listar docente
GET     /listaDocentes        controllers.HomeController.listaDocentes()

# CrearDocentes  
GET     /crearDocente           controllers.HomeController.crearDocenteGet()
POST    /crearDocente           controllers.HomeController.crearDocentesPost()



#Editar
GET     /editarDocentes/:id       controllers.HomeController.editarDocenteGet(id: Long)
POST    /editarDocentes/:id       controllers.HomeController.EditarDocentePost(id: Long)

#Eliminar 
GET     /eliminarDocente/:id       controllers.HomeController.EliminarDocente(id: Long)
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)


#Buscar..NUEVO
GET     /buscarEstudiante/:id       controllers.HomeController.BuscarDocenteGet(id: Long)
POST    /buscarEstudiante/:id       controllers.HomeController.BuscarDocentePost(id: Long)

